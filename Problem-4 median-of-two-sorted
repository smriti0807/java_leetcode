class Solution {
    public double findMedianSortedArrays(int[] nums1, int[] nums2) {
    int size = nums1.length+nums2.length;
    int[] arr= new int[size];
    int i,j;
    for( i=0;i<nums1.length;i++)
    {
        arr[i]=nums1[i];
    }
    for(j=0;j<nums2.length;j++)
    {
        arr[i]=nums2[j];
        i++;
    }
    Arrays.sort(arr);
    int a=size/2;
    int b=a-1;
     }
}
